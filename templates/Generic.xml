<templateSet group="Generic">
  <template name="__timestamp__" value="$timestamp$" description="" toReformat="false" toShortenFQNames="true">
    <variable name="timestamp" expression="timestamp()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="__date__" value="$date$" description="" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="date(&quot;yyyy-MM-dd'T'HH:mm:ssXXX&quot;)" defaultValue="date(&quot;yyyy-MM-dd'T'HH:mm:ssZ&quot;)" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="__date_short__" value="$date$" description="" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="date(&quot;dd-MM-yy&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>